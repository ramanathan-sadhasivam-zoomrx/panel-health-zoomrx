{
  "name": "panel-health-zoomrx",
  "version": "1.0.0",
  "description": "Panel Health Dashboard - Survey Experience Analytics & Insights",
  "main": "panel-health-server/src/server.js",
  "scripts": {
    "install:all": "npm install --prefix panel-health-client && npm install --prefix panel-health-server",
    "build": "node build-frontend.js",
    "start": "npm start --prefix panel-health-server",
    "dev:backend": "npm run dev --prefix panel-health-server",
    "dev:frontend": "npm run dev --prefix panel-health-client",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "npm run build --prefix panel-health-client",
    "test": "npm test --prefix panel-health-client && npm test --prefix panel-health-server"
  },
  "keywords": [
    "survey",
    "analytics",
    "dashboard",
    "nps",
    "experience",
    "react",
    "express",
    "mysql"
  ],
  "author": "Panel Health Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/panel-health-zoomrx.git"
  },
  "workspaces": [
    "panel-health-client",
    "panel-health-server"
  ]
} 